{
  "AllowedHosts": "*",
  "ApplicationSettings": {
    "DatabaseSetting": {
      "ConnectionStrings": {
        "SqlServer": "Server=.;Database=dotnetcore_vuejs;User Id=YourUsername;Password=YourPassword",
        "Postgres": "Host=localhost;Database=dotnetcore_vuejs;Username=YourUsername;Password=YourPassword"
      },
      "StoreFilesOnDatabase": true,
      "DatabaseProvider": 1 /* Postgres = 1 , SqlServer = 2 */
    },
    "IdentitySetting": {
      "PasswordRequireDigit": "true",
      "PasswordRequiredLength": "6",
      "PasswordRequireNonAlphanumeric": "false",
      "PasswordRequireUppercase": "false",
      "PasswordRequireLowercase": "false",
      "RequireUniqueEmail": "true"
    },
    "JwtSetting": {
      "SecretKey": "LongerThan-16Char-SecretKey",
      "EncryptKey": "16CharEncryptKey",
      "Issuer": "DotNetCore-VueJs",
      "Audience": "DotNetCore-VueJs",
      "NotBeforeMinutes": "0",
      "AccessTokenExpirationDays": "1",
      "RefreshTokenExpirationDays": "7"
    },
    "LogSetting": {
      "TableName": "SysError",
      "AutoCreateSqlTable": true,
      "MinimumLevelSerilog": "Error"
    },
    "MailSetting": {
      "EmailAddress": "email@email.com",
      "DisplayName": "DotNetCore-VueJs",
      "Password": "password",
      "SmtpServer": "smtp.gmail.com",
      "Port": 587
    }
  }
}